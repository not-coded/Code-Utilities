command /gamemode [<text>] [<player>]:
	usage: {@prefix} &6/gamemode <survival/creative/adventure/spectator> <player>
	permission: utilities.gamemode.all
	trigger:
		if arg-1 is not set:
			send "{@prefix} &6/gamemode <survival/creative/adventure/spectator> <player>"
		else if arg-1 is set:
			if arg-2 is not set:
				if arg-1 is "survival" or "s" or "0":
					set player's gamemode to survival
					send "{@prefix} &6Your gamemode has been set to &csurvival&6."
				if arg-1 is "creative" or "c" or "1":
					set player's gamemode to creative
					send "{@prefix} &6Your gamemode has been set to &ccreative&6."
				if arg-1 is "spectator" or "sp" or "3":
					set player's gamemode to spectator
					send "{@prefix} &6Your gamemode has been set to &cspectator&6."
				else if arg-1 is "adventure" or "a" or "2":
					set player's gamemode to adventure
					send "{@prefix} &6Your gamemode has been set to &cadventure&6."
			else if arg-2 is set:
				if arg-1 is "survival" or "s" or "0":
					set arg-2's gamemode to survival
					send "{@prefix} &6Your gamemode has been set to &csurvival&6." to arg-2
				if arg-1 is "creative" or "c" or "1":
					set arg-2's gamemode to creative
					send "{@prefix} &6Your gamemode has been set to &ccreative&6." to arg-2
				if arg-1 is "spectator" or "sp" or "3":
					set arg-2's gamemode to spectator
					send "{@prefix} &6Your gamemode has been set to &cspectator&6." to arg-2
				else if arg-1 is "adventure" or "a" or "2":
					set arg-2's gamemode to adventure
					send "{@prefix} &6Your gamemode has been set to &cadventure&6." to arg-2
					
